{
  "name": "base-up",
  "version": "0.21.0",
  "description": "A modern TypeScript utility library with numerous versatile functions, constants, and types",
  "keywords": [
    "stdlib",
    "type-level",
    "functional",
    "functional-programming",
    "immutable",
    "collection",
    "Array",
    "Iterable",
    "Map",
    "Set",
    "Record",
    "filter",
    "take",
    "drop",
    "takeLast",
    "dropLast",
    "shuffle",
    "sort",
    "join",
    "chunk",
    "groupBy",
    "permutation",
    "set-operation",
    "difference-set",
    "union-set",
    "intersection-set",
    "type-predicate",
    "assertion-function",
    "assert",
    "lexicographic-order",
    "pipe",
    "method-chaining",
    "curry",
    "modulo",
    "prime-test",
    "wheel-factorization",
    "nullish"
  ],
  "homepage": "https://github.com/ootidea/base-up#readme",
  "bugs": {
    "url": "https://github.com/ootidea/base-up/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ootidea/base-up.git"
  },
  "license": "CC0-1.0",
  "author": "ootidea",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vite",
    "format": "run-p format:*",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\"",
    "format:package-json": "sort-package-json",
    "postversion": "git push && git push --tags && npm publish ./",
    "prepublishOnly": "npm run build",
    "test": "vitest --silent=false",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "sort-package-json": "^2.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "volta": {
    "node": "18.12.1"
  }
}
