{
  "name": "base-up",
  "version": "0.8.0",
  "description": "A modern TypeScript utility library",
  "keywords": [
    "stdlib",
    "functional",
    "functional-programming",
    "immutable",
    "collection",
    "Array",
    "Iterable",
    "Map",
    "Set",
    "Record",
    "map",
    "filter",
    "shuffle",
    "sort",
    "join",
    "chunk",
    "groupBy",
    "permutation",
    "set-operation",
    "difference-set",
    "union-set",
    "intersection-set",
    "type-predicate",
    "type-assertion",
    "assert",
    "lexicographic-order",
    "pipe",
    "curry",
    "modulo"
  ],
  "license": "CC0-1.0",
  "author": "ootidea",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/base-up.js",
      "require": "./dist/base-up.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "./dist/base-up.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc -p tsconfig-emit.json",
    "dev": "vite",
    "format": "run-p format:*",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,scss}\"",
    "format:package-json": "sort-package-json",
    "postversion": "git push && git push --tags && npm publish ./",
    "prepublishOnly": "npm run build",
    "test": "vitest --silent=false",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "sort-package-json": "^2.4.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "volta": {
    "node": "18.12.1"
  }
}
